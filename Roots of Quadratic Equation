//WAP to print roots of quadratic equation

#include<stdio.h>
#include<math.h>
#include<conio.h>
void main()
{
int a,b,c,d,r1,r2;
clrscr();
printf("enter the value");
scanf("%d%d%d",&a,&b,&c);
d=b*b-4*a*c;
if(d>=0) {
r1=(-b+sqrt(d))/(2*a);
r2=(-b-sqrt(d))/(2*a);
printf("root1 =%d and root2 =%d",r1,r2);
}
else
{
printf("roots are imaginary");
}
getch();
}
